{
    "name": "nguyentranchung/laravel-metable",
    "description": "Fluent Meta Data for Eloquent Models, as if it is a property on your model.",
    "keywords": ["laravel", "laravel meta", "laravel metable", "model", "eloquent"],
    "authors": [
        {
            "name": "Nguyen Tran Chung",
            "homepage": "https://chungnguyen.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0|~5.1"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-0": {
            "NguyenTranChung\\Metable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NguyenTranChung\\Metable\\MetableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
